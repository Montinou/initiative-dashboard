{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=22 <25",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest --config=automation/config/vitest.config.ts",
    "test:ui": "vitest --ui --config=automation/config/vitest.config.ts",
    "test:run": "vitest run --config=automation/config/vitest.config.ts",
    "test:coverage": "vitest --coverage --config=automation/config/vitest.config.ts",
    "test:watch": "vitest --watch --config=automation/config/vitest.config.ts",
    "test:unit": "vitest run --config=automation/config/vitest.config.ts automation/unit",
    "test:integration": "vitest run --config=automation/config/vitest.config.ts automation/integration",
    "test:e2e": "playwright test --config=automation/config/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=automation/config/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=automation/config/playwright.config.ts --debug",
    "test:e2e:fema": "playwright test --config=automation/config/playwright.config.ts --project=fema-tenant",
    "test:e2e:siga": "playwright test --config=automation/config/playwright.config.ts --project=siga-tenant",
    "test:e2e:stratix": "playwright test --config=automation/config/playwright.config.ts --project=stratix-tenant",
    "test:e2e:mobile": "playwright test --config=automation/config/playwright.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:visual": "playwright test --config=automation/config/playwright.config.ts --project=visual-desktop",
    "test:visual:update": "playwright test --config=automation/config/playwright.config.ts --project=visual-desktop --update-snapshots",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:report": "playwright show-report automation/reports/playwright-report",
    "docs:serve": "cd docs && python3 -m http.server 3001",
    "docs:open": "open http://localhost:3001",
    "docs:watch": "node scripts/docs-watcher.js",
    "docs:validate": "node scripts/validate-docs.js",
    "docs:analyze": "node scripts/analyze-dependencies.js",
    "docs:build": "npm run docs:analyze && npm run docs:validate",
    "perf:analyze": "npm run build && npx webpack-bundle-analyzer .next/static/chunks/",
    "perf:build-analyze": "ANALYZE=true npm run build",
    "perf:monitor": "node scripts/performance-monitor.js",
    "db:migrate": "node scripts/run-migrations.js",
    "db:reset-cache": "node scripts/reset-materialized-views.js",
    "db:seed": "tsx scripts/seed-database.ts",
    "prepare": "husky"
  },
