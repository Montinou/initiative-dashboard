{
  "role": "Analyst",
  "description": "Read-only access to all strategic information and reporting. Ideal for control de gestión and report preparation without modification risks.",
  "permissions": {
    "viewDashboards": true,
    "manageUsers": false,
    "manageAreas": false,
    "createInitiatives": false,
    "editInitiatives": false,
    "exportData": true,
    "viewAllAreas": true,
    "deleteUsers": false,
    "deleteAreas": false,
    "deleteInitiatives": false,
    "accessAnalytics": true,
    "configureSystem": false,
    "filterData": true,
    "generateReports": true,
    "viewHistoricalData": true
